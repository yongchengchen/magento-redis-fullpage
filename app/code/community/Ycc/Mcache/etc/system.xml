<config>
    <sections>
	<dev>
	    <groups>
		<redis_cache>
			<label>Redis Full Page Cache</label>
                        <frontend_type>text</frontend_type>
                        <sort_order>55</sort_order>
                        <show_in_default>1</show_in_default>
                        <show_in_website>1</show_in_website>
                        <show_in_store>1</show_in_store>
                        <fields>
                                <static_html translate="label">
                                        <label>Cache Static Page(.html)</label>
                                        <frontend_type>select</frontend_type>
                                        <source_model>adminhtml/system_config_source_yesno</source_model>
                                        <sort_order>1</sort_order>
                                        <show_in_default>1</show_in_default>
                                        <show_in_website>1</show_in_website>
                                        <show_in_store>1</show_in_store>
                                </static_html>
                                <whitelist translate="label">
                                        <label>Cache White List</label>
					<frontend_type>TextArea</frontend_type>
                                        <sort_order>5</sort_order>
                                        <show_in_default>1</show_in_default>
                                        <show_in_website>1</show_in_website>
                                        <show_in_store>1</show_in_store>
					<comment><![CDATA[Put the url path to here, separated with ";"]]></comment>
                                </whitelist>
                                <append_js translate="label">
                                        <label>Append js for cached page</label>
					<frontend_type>TextArea</frontend_type>
                                        <sort_order>10</sort_order>
                                        <show_in_default>1</show_in_default>
                                        <show_in_website>1</show_in_website>
                                        <show_in_store>1</show_in_store>
					<comment><![CDATA[For cached page, you can add special javascript to run on these cached page.]]></comment>
                                </append_js>
                        </fields>
		</redis_cache>
	    </groups>
	</dev>
    </sections>
</config>
